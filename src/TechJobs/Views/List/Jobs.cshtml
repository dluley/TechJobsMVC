*/


    @foreach (Dictionary<string, string> job in ViewBag.jobs)
    {
        <table class="job-listing">

            @foreach (KeyValuePair<string, string> line in job)
            {
                <tr>
                    <td>@line.Key:</td>
                    <td>@line.Value</td>
                </tr>
            }
        </table>
    }
}